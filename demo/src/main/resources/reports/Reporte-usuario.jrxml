                    <textFieldExpression><![CDATA[new java.text.SimpleDateFormat("EEEE dd 'de' MMMM 'de' yyyy", new java.util.Locale("es", "ES")).format(new java.util.Date())]]></textFieldExpression>
                </textField>
                <staticText>
                    <reportElement x="10" y="6" width="100" height="20" forecolor="#030303"/>
                    <textElement verticalAlignment="Middle">
                        <font fontName="Arial" size="10" isBold="true"/>
                    </textElement>
                    <text><![CDATA[Fecha de emisión:]]></text>
                </staticText>
            </frame>
        </band>
    </title>

